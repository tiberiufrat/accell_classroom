<div class="alert alert-primary alert-has-icon" onclick="window.location='<%= form.url %>'" style="cursor: pointer">
  <div class="alert-icon"><i class="far fa-file-alt"></i></div>
  <div class="alert-body">
    <div class="alert-title"><%= form.title %></div>
    <span class="d-none d-md-block"><%= form.url.truncate(50) %></span>
  </div>
</div>
