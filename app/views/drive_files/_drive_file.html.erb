<div class="alert alert-success alert-has-icon" onclick="window.location='<%= drive_file.link %>'" style="cursor: pointer">
  <div class="alert-icon"><i class="fab fa-google-drive"></i></div>
  <div class="alert-body">
    <div class="alert-title"><%= drive_file.title %></div>
    <span class="d-none d-md-block"><%= drive_file.link.truncate(50) %></span>
  </div>
</div>
