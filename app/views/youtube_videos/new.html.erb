<section class="section">
  <div class="section-header d-mobile-none">
    <h1>Youtube Videos</h1>
    <div class="section-header-breadcrumb">
      <div class="breadcrumb-item"><%= link_to 'Dashboard', root_path %></div>
      <div class="breadcrumb-item"><%= link_to 'Youtube Videos', youtube_videos_path %></div>
      <div class="breadcrumb-item">New Youtube Video</div>
    </div>
  </div>
  <div class="section-body">
    <div class="card">
      <div class="card-header">
        <h4>New Youtube Video</h4>
      </div>
      <div class="card-body">
        <%= render 'form', youtube_video: @youtube_video %>
      </div>
    </div>
  </div>
</section>

<%= link_to icon('far fa-arrow-left', 'Back'), youtube_videos_path, class: 'btn btn-icon icon-left btn-secondary d-mobile-none' %>

<% content_for(:page_title, 'New Youtube Video') %>
