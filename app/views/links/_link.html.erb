<div class="alert alert-secondary alert-has-icon" onclick="window.location='<%= link.url %>'" style="cursor: pointer">
  <div class="alert-icon"><i class="far fa-external-link"></i></div>
  <div class="alert-body">
    <div class="alert-title"><%= link.title %></div>
    <span class="d-none d-md-block"><%= link.url.truncate(50) %></span>
  </div>
</div>
